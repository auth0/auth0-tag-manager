{
  "name": "auth0-tag-manager",
  "version": "2.0.7",
  "description": "A simple tag manager by Auth0",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production ./tools/scripts/build.sh",
    "build:dev": "NODE_ENV=development ./tools/scripts/build.sh",
    "start": "NODE_ENV=development webpack --config ./tools/webpack/webpack.playground.config.babel.js && http-server ./tools/playground",
    "test": "jest --ci --verbose",
    "test:development": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/auth0-tag-manager.git"
  },
  "keywords": [
    "tag",
    "manager"
  ],
  "author": "Alejo Fernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/auth0-tag-manager/issues"
  },
  "homepage": "https://github.com/auth0/auth0-tag-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "7.0.0",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "es6-promise": "^4.1.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.3.0",
    "http-server": "^0.9.0",
    "jest": "^20.0.4",
    "prettier-eslint": "^7.1.0",
    "semver": "^5.3.0",
    "webpack": "^2.3.3",
    "webpack-node-externals": "^1.5.4"
  },
  "files": [
    "build",
    "lib",
    "yarn.lock",
    "es5.js"
  ],
  "dependencies": {},
  "jest": {
    "globals": {
      "window": true,
      "document": true
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
