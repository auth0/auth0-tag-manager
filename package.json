{
  "name": "auth0-metrics-tag-manager",
  "version": "0.1.0",
  "description": "A simple tag manager for auth0-metrics",
  "main": "build/auth0-metrics-tag-manager.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./tools/webpack/webpack.config.babel.js",
    "build:dev": "NODE_ENV=development webpack --config ./tools/webpack/webpack.config.babel.js",
    "start": "NODE_ENV=development webpack --config ./tools/webpack/webpack.playground.config.babel.js && http-server ./tools/playground"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/auth0-metrics-tag-manager.git"
  },
  "keywords": [
    "tag",
    "manager"
  ],
  "author": "Alejo Fernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/auth0-metrics-tag-manager/issues"
  },
  "homepage": "https://github.com/auth0/auth0-metrics-tag-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "http-server": "^0.9.0",
    "webpack": "^2.3.3",
    "webpack-node-externals": "^1.5.4",
    "yaml-include-loader": "^1.0.0"
  },
  "dependencies": {
    "promiscuous": "^0.7.0"
  }
}
